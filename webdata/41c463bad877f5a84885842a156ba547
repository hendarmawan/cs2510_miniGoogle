  <script type="text/javascript">
var sections = [
	"home",
	"policy",
	"assignments",
	"mwlectures",
	"thlectures",
	"submission",
	"compare"
];

function showOnly(sectionName) {
	i = sectionName.indexOf(".");
	if(i != -1) {
		sectionName = sectionName.substring(0, i);
	}
	if(document.getElementById) {
		for(var s in sections) {
			s = sections[s];
			var section = document.getElementById("sec:" + s);
			if(s == sectionName) {
				section.style.display = "";
			} else {
				section.style.display = "none";
			}
		}
	}
}

function checkAnchor() {
	if(self.document.location.hash) {
		showOnly(self.document.location.hash.substring(1));
	} else {
		showOnly("home");
	}
}
window.onhashchange = checkAnchor;
  </script>
</head>
<body onload="checkAnchor()">
