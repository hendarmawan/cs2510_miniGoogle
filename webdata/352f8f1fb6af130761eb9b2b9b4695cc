<title>daniel mosse homepage</title><script language="javascript">
 
var lsr="layer.", lr, ss;
var coords="0,0, 0,30, 0,60, 0,80, 0,60, 0,30, 0,0";
var bigArray=new Array (250);
var arr;
var big;
var n, ie;
 
function init() {
	n = (document.layers) ? 1:0;
	ie = (document.all) ? 1:0;
	if (parseInt(navigator.appVersion) < 4) {
	} else if (n) {
		lr="document.layers";
		ss="";
	} else if (ie) {
		lr="document.all";
		ss=".style";
	}
	bigArray=coords.split(",");
	arr=new Array ("big");
	big = new animatedObject( 'big', 'a', 'no', 20, 0, '0', bigArray);
}

function show(name) { 
	eval(lr+'["'+name+'"]'+ss+'.visibility="visible"'); 
}
function hide(name) { 
	eval(lr+'["'+name+'"]'+ss+'.visibility="hidden"'); 
}

function animatedObject (object,name,loop,speed,index,end,route){
	this.objectName=object;
	this.name=name;
	this.loop=loop;
	this.speed=speed;
	this.selectedIndex=index;
	this.endRoutines=end;
	this.route=route;
}
 

var totalTime=10;
var currTime=0;
var timeline=new Array ();
timeline[2]='show("a");anim("big")';
		
function start(){
	currTime=0;
	control();
}
 
function control(){
	if (currTime <= totalTime){
		currTime++;
		if (timeline[currTime] != null){
			 eval(timeline[currTime]);
		}
		setTimeout("control()",1000);
	}
}
 
function move(name,left,top){
	eval(lr+'["'+name+'"]'+ss+'.top=top');
	eval(lr+'["'+name+'"]'+ss+'.left=left');
}
 
function anim(obj){ 
	if (eval(obj +'.selectedIndex <' +obj+ '.route.length')){
		eval('move(' +obj+ '.name,' +obj+ '.route[' +obj+ '.selectedIndex],' +obj+ '.route[' +obj+ '.selectedIndex+1])');
		eval (obj+'.selectedIndex+=2');
		setTimeout('anim("'+obj+'")',eval(obj+'.speed'));
	} else {
		var c=eval(obj+'.endRoutines');
		eval (c);
		eval (obj + '.selectedIndex=0');
		if (eval(obj+'.loop=="yes"')){
			eval('anim("'+obj+'")',eval(obj+'.speed'));
		}
	}
}
</script>
