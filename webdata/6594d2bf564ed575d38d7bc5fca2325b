<script type="text/javascript">
function handleHiddenTabs(max) {
	for (var i = 1; i <= max; i++) {
		var tab = document.getElementById("tab_" + i);
		if (tab && tab.firstChild.firstChild.innerHTML.search(/\(hidden\)/i) >= 0) {
			tab.style.display = "none";
		}
	}
}
handleHiddenTabs(11);
</script>

   
   




